    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openflying/cli.git",
    "directory": "packages/cli"
  },
  "author": {
    "name": "紫竹",
    "email": "luozhu2021@gmail.com",
    "url": "https://youngjuning.js.org"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/openflying/cli/issues"
  },
  "homepage": "https://github.com/openflying/cli/tree/main/#readme",
  "devDependencies": {
    "@luozhu/eslint-config-typescript": "^2.1.0",
    "@luozhu/prettier-config": "^1.2.0",
    "@types/ejs": "^3.1.2",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^20.4.1",
    "@types/ora": "^3.2.0",
    "@types/prompts": "^2.4.4",
    "eslint": "^7.32.0",
    "lint-staged": "^13.2.3",
    "prettier": "^2.3.2",
    "standard-version": "^6",
    "typescript": "^5.1.6",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "chalk": "4.1.1",
    "commander": "10.0.1",
    "cosmiconfig": "^8.2.0",
