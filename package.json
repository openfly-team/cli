    "download-npm-package": "^3.1.12",
    "ejs": "^3.1.9",
    "fs-extra": "^11.1.1",
    "isemail": "^3.2.0",
    "kolorist": "^1.8.0",
    "lodash.isequal": "^4.5.0",
    "ora": "5.4.1",
    "prompts": "^2.4.2",
    "recursive-readdir": "^2.2.3",
    "tmp-promise": "^3.0.3",
    "userhome": "^1.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
