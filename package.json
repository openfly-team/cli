{
  "name": "@openfly/cli",
  "version": "1.2.4",
  "description": "Common Frontend Scaffold",
  "keywords": [
    "cli",
    "fe",
    "scaffold"
  ],
  "files": [
    "dist",
    "template"
  ],
  "bin": {
